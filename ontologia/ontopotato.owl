        <Descripcion rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Este hongo afecta a las hojas y tallos.</Descripcion>
        <Nombre rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Phoma andina</Nombre>
        <NombreCientifico rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Phoma andina</NombreCientifico>
