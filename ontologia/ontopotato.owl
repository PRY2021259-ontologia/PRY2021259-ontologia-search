    <!-- http://localhost:3030/ontopotato/data#causa19 -->

    <owl:NamedIndividual rdf:about="http://localhost:3030/ontopotato/data#causa19">
        <rdf:type rdf:resource="http://localhost:3030/ontopotato/data#AgenteCausal"/>
        <es_agente_causal_de rdf:resource="http://localhost:3030/ontopotato/data#inf16"/>
        <Descripcion rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Las especies de este género contienen patógenos de plantas y agentes de deterioro de alimentos.</Descripcion>
        <Nombre rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ulocladium</Nombre>
        <NombreCientifico rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ulocladium</NombreCientifico>
        <Tipo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hongo</Tipo>
    </owl:NamedIndividual>
    


    <!-- http://localhost:3030/ontopotato/data#causa2 -->

    <owl:NamedIndividual rdf:about="http://localhost:3030/ontopotato/data#causa2">
        <rdf:type rdf:resource="http://localhost:3030/ontopotato/data#AgenteCausal"/>
        <es_agente_causal_de rdf:resource="http://localhost:3030/ontopotato/data#inf2"/>
        <Descripcion rdf:datatype="http://www.w3.org/2001/XMLSchema#string">La enfermedad es causada por el patógeno Phytophthora erythroseptica. Al igual que P. infestans y todos los otros Oomycetes, no es considerado como hongo, sino que está· incluido en el Reino Chromista. La colonia del patógeno en medio de cultivo Papa Dextrosa Agar (PDA) tiene un desarrollo arrosetado de color blanquecino. El patógeno se desarrolla en un rango de temperaturas de 2.5 a 34°C, siendo el óptimo 27.5°C.</Descripcion>
        <Nombre rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Phytophthora erythroseptica</Nombre>
        <NombreCientifico rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Phytophthora erythroseptica</NombreCientifico>
        <Tipo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Chromista</Tipo>
    </owl:NamedIndividual>
    


    <!-- http://localhost:3030/ontopotato/data#causa20 -->

    <owl:NamedIndividual rdf:about="http://localhost:3030/ontopotato/data#causa20">
        <rdf:type rdf:resource="http://localhost:3030/ontopotato/data#AgenteCausal"/>
        <es_agente_causal_de rdf:resource="http://localhost:3030/ontopotato/data#inf17"/>
        <Descripcion rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Es una especie de hongo de la roya que infecta a cultivos agrícolas como la papa y el tomate.</Descripcion>
        <Nombre rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Puccinia pittieriana</Nombre>
        <NombreCientifico rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Puccinia pittieriana</NombreCientifico>
        <Tipo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hongo</Tipo>
    </owl:NamedIndividual>
    


