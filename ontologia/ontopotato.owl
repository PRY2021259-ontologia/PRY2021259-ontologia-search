    <!-- http://localhost:3030/ontopotato/data#causa18 -->

    <owl:NamedIndividual rdf:about="http://localhost:3030/ontopotato/data#causa18">
        <rdf:type rdf:resource="http://localhost:3030/ontopotato/data#AgenteCausal"/>
        <es_agente_causal_de rdf:resource="http://localhost:3030/ontopotato/data#inf15"/>
        <Descripcion rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Este hongo afecta tallos y hojas de papa.</Descripcion>
        <Nombre rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Septoria lycopersici</Nombre>
        <NombreCientifico rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Septoria lycopersici</NombreCientifico>
